{
  "keybindings": {
    "Home": {
      "<q>": "Quit",
      "<Esc>": "Quit",
      "<Ctrl-d>": "Quit",
      "<Ctrl-c>": "Quit",
      "<Enter>": "Select",
      "<Up>": "MoveUp",
      "<Down>": "MoveDown",
      "<Home>": "MoveToTheFirst",
      "<End>": "MoveToTheLast"
    },
    "Cron": {
      "<q>": { "ChangeMode": "Home" },
      "<Esc>": { "ChangeMode": "Home" },
      "<Enter>": "Select",
      "<Up>": "MoveUp",
      "<Down>": "MoveDown",
      "<Home>": "MoveToTheFirst",
      "<End>": "MoveToTheLast",
      "<n>": "NewRecord",
      "<d>": "DeleteRecord"
    },
    "CronPopup": {
      "<Esc>": { "ChangeMode": "Cron" },
      "<Enter>": "Confirm",
      "<Tab>": "SwitchElement",
    },
    "MySql": {
      "<Esc>": { "ChangeMode": "Home" },
      "<Tab>": "SwitchElement",
      "<Up>": "MoveUp",
      "<Down>": "MoveDown",
    },
    "Users": {
      "<Esc>": { "ChangeMode": "Home" },
      "<Up>": "MoveUp",
      "<Down>": "MoveDown",
    },
    "Webserver": {
      "<Esc>": { "ChangeMode": "Home" },
    },
    "Settings": {
      "<Esc>": { "ChangeMode": "Home" },
    }
  },
  "settings": {
    "Cron": {
      "timezone": "Europe/Prague",
    },
    "Users": {
      "docroot": "/home",
    },    
  }
}
