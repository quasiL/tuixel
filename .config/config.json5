{
  "keybindings": {
    "Home": {
      "<q>": "Quit",
      "<Esc>": "Quit",
      "<Ctrl-d>": "Quit",
      "<Ctrl-c>": "Quit",
      "<Enter>": "Select",
      "<Up>": "MoveUp",
      "<Down>": "MoveDown",
